# Touch of Terra Dashboard - Frontend Environment Configuration

# API URLs
VITE_API_URL=http://localhost:5001
VITE_BACKEND_URL=http://localhost:5001

# External Data Sources (Optional - fallback data available)
# VITE_HUD_API_KEY=
# VITE_LOUISVILLE_API_KEY=
# VITE_DATAGOV_API_KEY=

# Google Maps API (Optional - falls back to OpenStreetMap)
# VITE_GOOGLE_MAPS_API_KEY=

# OpenWeather API (Optional)
# VITE_OPENWEATHER_API_KEY=

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_FORECASTING=true
VITE_ENABLE_OFFLINE_MODE=true
VITE_ENABLE_EXPORT=true

# Data Refresh Intervals (milliseconds)
VITE_DATA_REFRESH_INTERVAL=3600000
VITE_WEATHER_REFRESH_INTERVAL=1800000

# Environment
VITE_NODE_ENV=development
