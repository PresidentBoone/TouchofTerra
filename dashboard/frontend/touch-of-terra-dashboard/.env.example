# Touch of Terra Dashboard - Environment Configuration
# Copy this file to .env and fill in your actual values

# API URLs
VITE_API_URL=http://localhost:5001
VITE_BACKEND_URL=http://localhost:5001

# External Data Sources
VITE_HUD_API_KEY=your_hud_api_key_here
VITE_HUD_API_URL=https://www.hudexchange.info/resource/reportmanagement/published/CoC_PopSub_NatlTerrDC_2024.xlsx

# Louisville Metro Open Data
VITE_LOUISVILLE_DATA_URL=https://data.louisvilleky.gov/api
VITE_LOUISVILLE_API_KEY=your_louisville_api_key_here

# Data.gov API
VITE_DATAGOV_API_KEY=your_datagov_api_key_here
VITE_DATAGOV_API_URL=https://catalog.data.gov/api/3/action

# Google Maps API (for directions and geocoding)
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# OpenWeather API (for weather alerts)
VITE_OPENWEATHER_API_KEY=your_openweather_api_key_here

# Analytics
VITE_ENABLE_ANALYTICS=true

# Feature Flags
VITE_ENABLE_FORECASTING=true
VITE_ENABLE_OFFLINE_MODE=true
VITE_ENABLE_EXPORT=true

# Data Refresh Intervals (in milliseconds)
VITE_DATA_REFRESH_INTERVAL=3600000
VITE_WEATHER_REFRESH_INTERVAL=1800000

# Environment
VITE_NODE_ENV=development
